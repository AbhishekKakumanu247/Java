import javax.swing.*; 

class JLabelExample
{  
public static void main(String args[])  
    {  
JFrame f= new JFrame("JLabel Example"); 

JLabel l1,l2,l3;  

    l1=new JLabel("K.");  
    l1.setBounds(50,50, 100,30);  

    l2=new JLabel("Abhishek.");  
    l2.setBounds(60,60, 100,30); 

    l3=new JLabel("Welcomes you");  
    l3.setBounds(70,70, 100,30); 

f.add(l1); 
f.add(l2); 
f.add(l3);

f.setSize(300,300);  
f.setLayout(null); 
f.setVisible(true);  
    }  
    } 
